<%- include("./components/_header") -%>
<form class="pt-3 p-4" method="POST" action="admin" enctype="multipart/form-data">
    <div class="form-group">
        <label>Nome do produto</label>
        <input name="Title" type="text" class="form-control" placeholder="Nome do Produto">
    </div>
    <div class="form-group d-flex flex-row justify-content-around">
       <div>
           <label>Preço</label>
            <input step="any" name="Value" type="number" f class="form-control" placeholder="Preço">
       </div>
       <div>
            <label>Imagem do Produto</label>
            <input name="Img" type="file" class="form-control-file" placeholder="Imagem do Produto">
       </div>
        
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
<%- include("./components/_footer") -%>